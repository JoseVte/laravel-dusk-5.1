{
    "name": "josrom/laravel-dusk-5.1",
    "description": "Laravel Dusk provides simple end-to-end testing and browser automation.",
    "keywords": [
        "laravel",
        "testing",
        "webdriver",
        "laravel-5.1",
        "dusk"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Jose Vicente Orts Romero",
            "email": "vince@square1.io"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.1.*",
        "facebook/webdriver": "1.*",
        "nesbot/carbon": "~1.20"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Dusk\\": "src/"
        },
        "files": [
            "src/Http/helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
