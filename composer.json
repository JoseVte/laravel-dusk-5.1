{
    "name": "josrom/laravel-dusk-5.1",
    "description": "Laravel Dusk provides simple end-to-end testing and browser automation.",
    "keywords": ["laravel", "testing", "webdriver"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "Jose Vicente Orts Romero",
            "email": "vince@square1.io"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.1.*",
        "facebook/webdriver": "~1.0",
        "nesbot/carbon": "~1.20"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^0.9.6"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\Dusk\\": "src/"
        },
        "files": [
            "src/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/DuskTestCase.php",
            "tests/Browser/CreatesApplication.php"
        ]
    },
    "minimum-stability": "dev"
}
